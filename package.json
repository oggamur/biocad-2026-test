{
  "name": "biocad-2026-test",
  "version": "1.0.0",
  "description": "Dashboard and Analytics web application",
  "main": "server.js",
  "scripts": {
    "prestart": "npm run build:css",
    "start": "node server.js",
    "dev": "concurrently \"npm run watch:css\" \"npm start\"",
    "build:css": "sass scss/main.scss public/css/styles.css",
    "watch:css": "sass --watch scss/main.scss:public/css/styles.css"
