    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^4.18.2",
    "flatpickr": "^4.6.13"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "sass": "^1.69.5"
  }
}
